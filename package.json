  "repository": "github:dreamonkey/quasar-app-extension-meta",
  "bugs": "https://github.com/dreamonkey/quasar-app-extension-meta/issues",
  "engines": {
    "node": ">= 8.9.0",
    "npm": ">= 5.6.0",
    "yarn": ">= 1.6.0"
  },
  "devDependencies": {
    "@babel/types": "^7.15.6",
    "@types/node": "^12.20.27",
    "quasar": "^2.1.0",
    "typescript": "^4.4.3",
    "vue": "^3.2.19",
    "vue-i18n": "^9.1.0",
    "vue-router": "^4.0.11"
  },
  "publishConfig": {
    "access": "public"
  }
}
